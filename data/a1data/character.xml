<?xml version="1.0"?>
<!-- hierachrical definition of a character -->
<doc>
    <node type="free" name="root" position="0 -1.0 -0.2">
		<node type="rotary" name="main_y" position="0 0 0" axis="0 1 0" rotation="50">
    		<geom type="box" name="body" center="0 1 0" scale="1 0.75 2"/>
			<node type="rotary" name="lower_neck_x" position="0 1.1 0.75" axis="1 0 0" rotation="25">
				<geom type="box" name="neck" center="0 0.6 0" scale="0.3 1 0.5"/>
				<node type="rotary" name="upper_neck_x" position="0 1.1 0" axis="1 0 0" rotation="20">
					<geom type="box" name="head" center="0 0 0.25" scale="0.6 0.35 1"/>

					<geom type="sphere" name="left_eye" center="0.3 0 0.1" scale="0.1 0.1 0.1"/>
					<geom type="sphere" name="right_eye" center="-0.3 0 0.1" scale="0.1 0.1 0.1"/>

					<geom type="box" name="left_antler" center="0.2 0.175 -0.1" scale="0.1 0.5 0.1" rotation="0 0 -30"/>
					<geom type="box" name="right_antler" center="-0.2 0.175 -0.1" scale="0.1 0.5 0.1" rotation="0 0 30"/>

				</node>
			</node>
			<node type="rotary" name="tail_x" position="0 1.25 -0.9" axis="-1 0 0" rotation="50">
				<geom type="box" name="tail" center="0 0 -0.2" scale="0.2 0.2 0.5"/>
			</node>
		</node>
	</node>
</doc>